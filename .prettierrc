{
  "printWidth": 100,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "importOrder": [
    "^react$",
    "<THIRD_PARTY_MODULES>",
    "^@/ui(/.*)?$",
    "^@/custom(/.*)?$",
    "^@/providers(/.*)?$",
    "^@/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "endOfLine": "lf",
  "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
